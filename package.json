{
  "version": "1.0.2",
  "name": "gcc-min",
  "description": "Transpile and Minify ECMAScript 6 using Google Closure Compiler",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/gcc-min"
  },
  "author": "@ndaidong",
  "bin": {
    "gccmin": "./bin/index"
  },
  "main": "./index.js",
  "engines": {
    "node": ">= 6.0"
  },
  "scripts": {
    "lint": "eslint ./src ./test",
    "test": "npm run lint && snyk test && tape test/start.js | tap-spec",
    "coverage": "nyc tape test/start.js | tap-spec",
    "report": "npm run coverage && nyc report --reporter=lcov",
    "coveralls": "npm run report && cat ./coverage/lcov.info | coveralls",
    "snyk": "snyk wizard"
  },
  "dependencies": {
    "bellajs": "^5.4.0",
    "google-closure-compiler-js": "^20160828.0.1",
    "minimist": "^1.2.0",
    "promise-wtf": "^1.0.0"
  },
  "devDependencies": {
    "coveralls": "latest",
    "eslint": "latest",
    "nyc": "latest",
    "snyk": "latest",
    "tap-spec": "latest",
    "tape": "latest"
  },
  "keywords": [
    "compile",
    "minify",
    "es6",
    "util"
  ],
  "license": "MIT",
  "maintainers": [
    "ndaidong <ndaidong@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/ndaidong/gcc-min/issues"
  },
  "directories": {
    "test": "test"
  }
}
