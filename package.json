{
  "version": "2.0.0",
  "name": "gcc-min",
  "description": "Transpile and Minify ECMAScript 6 using Google Closure Compiler",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/gcc-min"
  },
  "author": "@ndaidong",
  "bin": {
    "gccmin": "./bin/index"
  },
  "main": "./index.js",
  "engines": {
    "node": ">= 6.0"
  },
  "scripts": {
    "lint": "eslint ./src ./test",
    "pretest": "npm run lint && snyk test",
    "tape": "nyc tape test/start.js | tap-spec",
    "test": "npm run tape",
    "posttest": "nyc report --reporter=lcov",
    "coveralls": "npm test && cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "babel-core": "6.x.x",
    "babel-plugin-transform-remove-strict-mode": "0.x.x",
    "babel-preset-env": "0.x.x",
    "bellajs": "6.x.x",
    "minimist": "1.x.x",
    "promise-wtf": "1.x.x",
    "shift-codegen": "5.x.x",
    "shift-parser": "5.x.x"
  },
  "devDependencies": {
    "coveralls": "latest",
    "eslint": "latest",
    "eslint-config-ggc": "latest",
    "nyc": "latest",
    "snyk": "latest",
    "tap-spec": "latest",
    "tape": "latest"
  },
  "keywords": [
    "compile",
    "minify",
    "es6",
    "util"
  ],
  "license": "MIT",
  "maintainers": [
    "ndaidong <ndaidong@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/ndaidong/gcc-min/issues"
  },
  "directories": {
    "test": "test"
  }
}
