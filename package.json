{
  "version": "1.0.8",
  "name": "gcc-min",
  "description": "Transpile and Minify ECMAScript 6 using Google Closure Compiler",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/gcc-min"
  },
  "author": "@ndaidong",
  "bin": {
    "gccmin": "./bin/index"
  },
  "main": "./index.js",
  "engines": {
    "node": ">= 6.0"
  },
  "scripts": {
    "lint": "eslint ./src ./test",
    "pretest": "npm run lint && snyk test",
    "test": "nyc tape test/start.js | tap-spec",
    "posttest": "nyc report --reporter=lcov",
    "coveralls": "npm test && cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "bellajs": "5.*.*",
    "google-closure-compiler-js": "latest",
    "minimist": "1.*.*",
    "promise-wtf": "1.*.*"
  },
  "devDependencies": {
    "coveralls": "latest",
    "eslint": "latest",
    "eslint-config-ggc": "latest",
    "nyc": "latest",
    "snyk": "latest",
    "tap-spec": "latest",
    "tape": "latest"
  },
  "keywords": [
    "compile",
    "minify",
    "es6",
    "util"
  ],
  "license": "MIT",
  "maintainers": [
    "ndaidong <ndaidong@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/ndaidong/gcc-min/issues"
  },
  "directories": {
    "test": "test"
  }
}
