{
  "version": "7.1.5",
  "name": "gcc-min",
  "description": "Just write scripts for Node.js environment, then build to use in the browsers",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/gcc-min"
  },
  "author": "@ndaidong",
  "bin": {
    "gccmin": "builder/build.js"
  },
  "main": "./index.js",
  "engines": {
    "node": ">= 8.6"
  },
  "scripts": {
    "lint": "eslint ./src ./test",
    "pretest": "npm run lint && npm run build",
    "test": "DEBUG=gccmin:* tap test/start.js --coverage --reporter=spec",
    "build": "DEBUG=gccmin:* node builder/build -e ../test/data/main.js -n xMath -f math -o ../test/data/output -p ../test/data/package.json",
    "reset": "node reset"
  },
  "dependencies": {
    "bellajs": "^8.0.0",
    "debug": "^4.1.1",
    "minimist": "^1.2.0",
    "rollup": "^1.21.2",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "terser": "^4.3.1"
  },
  "devDependencies": {
    "eslint-config-goes": "^1.1.6",
    "tap": "^14.6.2"
  },
  "keywords": [
    "compile",
    "minify",
    "es6",
    "util"
  ],
  "license": "MIT"
}
