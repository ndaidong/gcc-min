{
  "version": "3.0.21",
  "name": "gcc-min",
  "description": "Just write scripts for Node.js environment, then build to use in the browsers",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/gcc-min"
  },
  "author": "@ndaidong",
  "bin": {
    "gccmin": "./bin/index"
  },
  "main": "./index.js",
  "engines": {
    "node": ">= 6.0"
  },
  "scripts": {
    "lint": "eslint ./src ./test",
    "nsp": "nsp check --output checkstyle",
    "pretest": "npm run lint && npm run nsp",
    "tape": "nyc tape test/start.js | tap-spec",
    "test": "npm run tape",
    "posttest": "nyc report --reporter=lcov",
    "coveralls": "npm test && cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "babel-core": "6.x.x",
    "babel-plugin-transform-remove-strict-mode": "0.x.x",
    "babel-preset-env": "0.x.x",
    "bellajs": "6.x.x",
    "shift-codegen": "5.x.x",
    "shift-parser": "5.x.x"
  },
  "devDependencies": {
    "coveralls": "2.x.x",
    "eslint": "3.x.x",
    "eslint-config-ggc": "1.x.x",
    "nsp": "2.x.x",
    "nyc": "10.x.x",
    "tap-spec": "4.x.x",
    "tape": "4.x.x"
  },
  "keywords": [
    "compile",
    "minify",
    "es6",
    "util"
  ],
  "license": "MIT",
  "gccmin": {
    "source": "./test/data/sample-math.js",
    "target": "./test/data/output",
    "filename": "sample",
    "globalVar": "sampleMath"
  }
}
