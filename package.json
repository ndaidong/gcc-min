{
  "version": "6.1.0",
  "name": "gcc-min",
  "description": "Just write scripts for Node.js environment, then build to use in the browsers",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/gcc-min"
  },
  "author": "@ndaidong",
  "bin": {
    "gccmin": "builder/build.js"
  },
  "main": "./index.js",
  "engines": {
    "node": ">= 7.6"
  },
  "scripts": {
    "lint": "eslint ./src ./test",
    "nsp": "nsp check --output checkstyle",
    "report": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "pretest": "npm run lint && npm run nsp && npm run build",
    "tape": "nyc tape test/start.js | tap-spec",
    "test": "DEBUG=gccmin:* npm run tape",
    "posttest": "CODECOV_TOKEN=b7ad5ba4-6a46-4d1c-848f-f9011c18e16f npm run report",
    "build": "DEBUG=gccmin:* node builder/build -e ../test/data/main.js -n xMath -f math -o ../test/data/output -p ../test/data/package.json",
    "reset": "node reset"
  },
  "dependencies": {
    "bellajs": "^7.2.2",
    "debug": "^3.1.0",
    "minimist": "^1.2.0",
    "rollup": "^0.59.1",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-cleanup": "^3.0.0-beta.1",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "uglify-es": "^3.3.9"
  },
  "devDependencies": {
    "codecov": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-goes": "^1.0.0",
    "nsp": "^3.2.1",
    "nyc": "^11.7.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.9.0"
  },
  "keywords": [
    "compile",
    "minify",
    "es6",
    "util"
  ],
  "license": "MIT"
}
