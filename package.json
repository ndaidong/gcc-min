{
  "version": "4.1.0",
  "name": "gcc-min",
  "description": "Just write scripts for Node.js environment, then build to use in the browsers",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/gcc-min"
  },
  "author": "@ndaidong",
  "bin": {
    "gccmin": "./builder/index"
  },
  "main": "./index.js",
  "engines": {
    "node": ">= 7.6"
  },
  "scripts": {
    "lint": "eslint ./src ./tests",
    "nsp": "nsp check --output checkstyle",
    "report": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "pretest": "npm run lint && npm run nsp",
    "tape": "npm run build && nyc tape tests/start.js | tap-spec",
    "test": "npm run tape",
    "posttest": "CODECOV_TOKEN=b7ad5ba4-6a46-4d1c-848f-f9011c18e16f npm run report",
    "reset": "node bin/reset",
    "build": "node ./builder/index -e ../tests/data/main.js -n math -o ../tests/data/output -p ../tests/data/package.json"
  },
  "dependencies": {
    "babel-plugin-external-helpers": "6.x.x",
    "babel-preset-es2015-rollup": "3.x.x",
    "bellajs": "7.x.x",
    "minimist": "1.x.x",
    "rollup": "0.x.x",
    "rollup-plugin-babel": "2.x.x",
    "rollup-plugin-cleanup": "1.x.x",
    "rollup-plugin-commonjs": "8.x.x",
    "rollup-plugin-node-resolve": "3.x.x",
    "uglify-js": "3.x.x"
  },
  "devDependencies": {
    "codecov": "2.x.x",
    "eslint": "3.x.x",
    "eslint-config-ggc": "1.x.x",
    "nsp": "2.x.x",
    "nyc": "11.x.x",
    "tap-spec": "4.x.x",
    "tape": "4.x.x"
  },
  "keywords": [
    "compile",
    "minify",
    "es6",
    "util"
  ],
  "license": "MIT"
}
